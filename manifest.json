{
    "name": "antishorts",
    "description": "disables youtube shorts",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/shorts/*"],
        "js": ["index.js"],
        "run_at": "document_start"
      }
    ],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }